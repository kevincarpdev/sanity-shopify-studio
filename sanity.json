{
  "root": true,
  "api": {
    "projectId": "ropsozzs",
    "dataset": "production"
  },
  "project": {
    "name": "Sanity + Shopify demo"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/components",
    "@sanity/dashboard",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "media",
    "dashboard-widget-shopify",
    "@sanity/color-input",
    "hotspot-array"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "name": "part:@sanity/desk-tool/structure",
      "path": "./deskStructure.js"
    },
    {
      "implements": "part:@sanity/form-builder/input/image/asset-sources",
      "path": "./parts/assetSources.js"
    },
    {
      "implements": "part:@sanity/form-builder/input/file/asset-sources",
      "path": "./parts/assetSources.js"
    },
    {
      "implements": "part:@sanity/dashboard/config",
      "path": "./parts/dashboardConfig.js"
    },
    {
      "name": "part:@sanity/base/new-document-structure",
      "path": "./parts/newDocumentStructure.js"
    },
    {
      "implements": "part:@sanity/base/document-actions/resolver",
      "path": "./parts/resolveDocumentActions.js"
    }
  ]
}
